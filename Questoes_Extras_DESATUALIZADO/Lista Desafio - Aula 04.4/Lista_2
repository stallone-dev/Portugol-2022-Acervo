// Seção de Declarações
var

num: inteiro

inicio

//Mostra na tela a funcionalidade do programa
escreval ("Este programa mostra todos os números impares de zero até vinte")

num <- 1
//Laço de repetição
enquanto (num<20) faca
   escreval (num)
   num <- num + 2
fimenquanto

fimalgoritmo
